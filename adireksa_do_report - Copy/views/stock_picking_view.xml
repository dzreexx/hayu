<odoo>
     <record id="stock_picking_note_form_view_inherit" model="ir.ui.view">
            <field name="name">stock.picking.note.form.view.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
               <xpath expr="//field[@name='origin']" position="after">
                    <field name="note"/>
               </xpath>
            </field>
     </record>
</odoo>