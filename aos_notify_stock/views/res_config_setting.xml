<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.stock.notif</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <div id="production_lot_info" position="after">
                    <h2>Batas Atas &amp; Batas Bawah</h2>
                    <div class="row mt16 o_settings_container" name="batas_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box"
                            id="batas_setting">
                            <div class="o_setting_right_pane">
                                <label for="batas_atas"/>
                                <!-- <a href="https://www.odoo.com/documentation/15.0/applications/inventory_and_mrp/inventory/management/reporting/integrating_landed_costs.html" title="Documentation" class="o_doc_link" target="_blank"></a> -->
                                <div class="text-muted">
                                    Add Batas Atas Untuk QTY.
                                </div>
                                <div>
                                    <field name="batas_atas" class="oe_inline" style="vertical-align:baseline"/> %
                                </div>
                                <div class="content-group">
                                    <div name="batas_atas"/>
                                </div>
                            </div>
                        </div>
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_right_pane">
                                <label for="batas_bawah"/>
                                <div class="text-muted">
                                    Add Batas Bawah Untuk QTY.
                                </div>
                                <div>
                                    <field name="batas_bawah" class="oe_inline" style="vertical-align:baseline"/> %
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </field>
        </record>
    </data>
</odoo>
