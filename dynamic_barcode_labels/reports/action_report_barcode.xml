<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--  Price Tag -->
        <record id="barcode_price_tag_paper_format" model="report.paperformat">
            <field name="name">Barcode Price Tag Label Paper Format</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field> 
            <field name="orientation">Portrait</field>
            <field name="margin_top">5</field> 
            <field name="margin_bottom">5</field>  
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">37</field> 
            <field name="dpi">110</field>
        </record>

        <!-- action report barcode label mini -->
        <record id="action_report_dynamic_label_mini" model="ir.actions.report">
          <field name="name">Dynamic Label Mini</field>
          <field name="model">dynamic.label.wizard</field>
          <field name="report_type">qweb-pdf</field>
          <field name="report_name">dynamic_barcode_labels.report_dynamic_label_mini</field>
          <field name="report_file">dynamic_barcode_labels.report_dynamic_label_mini</field>
          <field name="paperformat_id" ref="dynamic_barcode_labels.barcode_price_tag_paper_format"/>
        </record>

        <!-- action report barcode label -->
        <record id="action_report_dynamic_label" model="ir.actions.report">
          <field name="name">Dynamic Label</field>
          <field name="model">dynamic.label.wizard</field>
          <field name="report_type">qweb-pdf</field>
          <field name="report_name">dynamic_barcode_labels.report_dynamic_label</field>
          <field name="report_file">dynamic_barcode_labels.report_dynamic_label</field>
          <field name="paperformat_id" ref="dynamic_barcode_labels.barcode_price_tag_paper_format"/>
        </record>


