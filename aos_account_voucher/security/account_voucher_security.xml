<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
    
    	<record id="group_accounting_account_voucher" model="res.groups">
	        <field name="name">Journal Voucher</field>
	        <field name="implied_ids" eval="[(4, ref('account.group_account_invoice'))]"/>
	        <field name="users" eval="[(4, ref('base.user_admin')),(4, ref('base.user_root'))]"/>
	        <field name="category_id" ref="base.module_category_hidden"/>
	    </record>
	    
        <record id="aos_voucher_comp_rule" model="ir.rule">
            <field name="name">Voucher multi-company</field>
            <field name="model_id" ref="model_account_voucher"/>
            <field eval="True" name="global"/>
            <field name="domain_force">[('company_id', 'in', company_ids)]</field>
        </record>
        
        <record id="aos_voucher_line_comp_rule" model="ir.rule">
            <field name="name">Voucher Line multi-company</field>
            <field name="model_id" ref="model_account_voucher_line"/>
            <field eval="True" name="global"/>
            <field name="domain_force">['|',('company_id','=',False),('company_id','in',company_ids)]</field>
        </record>
        
        <record id="account_voucher_can_validate" model="res.groups">
            <field name="name">Can Validate Voucher</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
        <record id="account_voucher_crosscheck_validate" model="res.groups">
            <field name="name">Crosscheck Voucher</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
        <record id="account_voucher_dashboard_view" model="res.groups">
            <field name="name">Dashboard Voucher</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>
    </data>
</odoo>
