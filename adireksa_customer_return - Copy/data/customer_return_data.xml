<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Customer Return sequences -->
        <record id="sequence_customer_return_adireksa" model="ir.sequence">
            <field name="name">Customer Return sequence Adireksa</field>
            <field name="code">customer.return.sequence</field>
            <field name="prefix">CR/ADI/%(range_year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">3</field>
        </record>
        
        <record id="sequence_customer_return_annapurna" model="ir.sequence">
