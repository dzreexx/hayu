<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <data>
               <record id="paperformat_a4_narrow_top" model="report.paperformat">
               <field name="name">A4 Narrow Top</field>
               <field name="default" eval="True" />
               <field name="format">A4</field>
               <field name="page_height">0</field>
               <field name="page_width">0</field>
               <field name="orientation">Portrait</field>
               <field name="margin_top">43</field>
               <field name="margin_bottom">30</field>
               <field name="margin_left">7</field>
               <field name="margin_right">7</field>
               <field name="header_line" eval="False"/>
               <field name="header_spacing">37</field>
               <field name="dpi">90</field>
          </record>

          <record id="action_report_adireksa_original_invoice" model="ir.actions.report">
               <field name="name">Original Invoice</field>
               <field name="model">account.move</field>
               <field name="report_type">qweb-pdf</field>
               <field name="report_name">adireksa_invoice_print.report_invoice_adireksa</field>
               <field name="report_file">adireksa_invoice_print.report_invoice_adireksa</field>
               <field name="print_report_name">'%s' % object.name</field>
               <field name="binding_model_id" ref="account.model_account_move"/>
               <field name="paperformat_id" ref="adireksa_invoice_print.paperformat_a4_narrow_top"/>
               <field name="binding_type">report</field>
               <field name="groups_id" eval="[(4, ref('account.group_account_invoice')),
                    (4, ref('account.group_account_readonly'))]"/>
          </record>


          <record id="action_report_adireksa_original_invoice_copy" model="ir.actions.report">
               <field name="name">Original Invoice Copy</field>
               <field name="model">account.move</field>
               <field name="report_type">qweb-pdf</field>
               <field name="report_name">adireksa_invoice_print.report_invoice_adireksa_copy</field>
               <field name="report_file">adireksa_invoice_print.report_invoice_adireksa_copy</field>
               <field name="print_report_name">'%s' % object.name</field>
               <field name="paperformat_id" ref="adireksa_invoice_print.paperformat_a4_narrow_top"/>
               <field name="groups_id" eval="[(4, ref('account.group_account_invoice')),
                    (4, ref('account.group_account_readonly'))]"/>
               <!-- <field name="binding_model_id" ref="account.model_account_move"/>
               <field name="binding_type">report</field>  -->
          </record>
           
