    <record id="cron_set_target_for_data_import" model="ir.cron">
		<field name="name">Set Data Target Group For Data Import</field>
		<field name="model_id" ref="model_res_partner"/>
		<field name="state">code</field>
		<field name="code">model._action_set_group_target()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="active">0</field>
		<field name="doall" eval="False" />
	</record>

    <record id="cron_set_customer_target_accounting" model="ir.cron">
		<field name="name">Set Data Target Group For Accounting</field>
		<field name="model_id" ref="model_account_move"/>
		<field name="state">code</field>
		<field name="code">model._action_set_group_target()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="active">0</field>
		<field name="doall" eval="False" />
	</record>

    <record id="cron_approve_for_cashback" model="ir.cron">
		<field name="name">Cashback Approve</field>
		<field name="model_id" ref="model_cashback_lines"/>
		<field name="state">code</field>
		<field name="code">model._action_approve_cashback()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="active">0</field>
		<field name="doall" eval="False" />
	</record>

    <record id="cron_fetch_data_cashback" model="ir.cron">
		<field name="name">Fetch Data Cashback</field>
		<field name="model_id" ref="model_adireksa_customer_target"/>
		<field name="state">code</field>
		<field name="code">model.fetch_customer_target()</field>
		<field name="interval_number">1</field>
		<field name="interval_type">days</field>
		<field name="numbercall">-1</field>
		<field name="active">0</field>
		<field name="doall" eval="False" />
	</record>
</odoo>
