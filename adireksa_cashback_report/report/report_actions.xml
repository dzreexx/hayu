<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--  Price Tag -->
        <record id="cashback_report_paper_format" model="report.paperformat">
            <field name="name">Cashback Report Label Paper Format</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">10</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">39</field>
            <field name="dpi">110</field>
        </record>

        <!-- action report cashback -->
        <record id="action_report_cashback" model="ir.actions.report">
          <field name="name">Print Report</field>
          <field name="model">cashback.print</field> 
          <field name="report_type">qweb-pdf</field>
          <field name="report_name">adireksa_cashback_report.cashback_report_template</field>
          <field name="report_file">adireksa_cashback_report.cashback_report_template</field>
          <field name="paperformat_id" ref="adireksa_cashback_report.cashback_report_paper_format"/>
        </record>

    </data>
</odoo>